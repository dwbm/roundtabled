<!DOCTYPE HTML>
<html>
	<head>
	<meta charset=utf-8>
	<title><%= title %></title>
	<%= csrf_meta_tag %>
	
	<meta name="description" content="bring everything to the table">
	<meta name="author" content="Daniel WB Mazzone">
	
	<%= render 'layouts/stylesheets' %>

	</head>

<body>

<section class="content">

	<%= image_tag("Logo.png", :alt => "RoundTabled", :class => "logo") %>

	<% flash.each do |key, value| %>
		<div class="flash <%= key %>"><%= value %></div>
	<% end %>

	<%= yield %>
	
</section>

<%= render 'layouts/footer' %>
<%= debug(params) if Rails.env.development? %>

</body>
</html>